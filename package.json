{
  "name": "editorjs-inline-image",
  "version": "2.2.2",
  "keywords": [
    "tool",
    "image",
    "server-less",
    "editor.js",
    "editorjs"
  ],
  "description": "Unsplash inline images tool for editorjs",
  "license": "MIT",
  "repository": "https://github.com/kommitters/editorjs-inline-image",
  "main": "./dist/bundle.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development --watch",
    "test": "jest",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src/ test/ --ext .js",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint src/ test/ --ext .js --fix"
  },
  "author": {
    "name": "kommitters Open Source",
    "email": "oss@kommit.co"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.10.1",
    "@testing-library/jest-dom": "^6.0.0",
    "babel-jest": "^30.0.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.0.0",
    "eslint": "9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^29.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "nock": "^14.0.0",
    "style-loader": "^4.0.0",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^6.0.0"
  },
  "dependencies": {
    "axios": "1.13.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/config/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.(css|svg)$": "<rootDir>/test/config/assetsTransform.js"
    }
  }
}
